{
  "name": "weblogout",
  "version": "1.0.0",
  "description": "Lambda function that handles user logout by invalidating jwt token.",
  "main": "index.js",
  "scripts": {
    "build": "npm run prune && npm run zip",
    "prune": "(cd ../../authCommon && npm prune --production) && npm prune --production",
    "zip": "rm -f ./function.zip && zip -r ./function.zip .  -x './src/test/*' -x '*.gitignore' -x 'coverage/*' -x '.nyc_output/*'",
    "sonar": "node sonarqube-scanner.js -Dsonar.login=${SONAR_TOKEN}",
    "test-build": "npm run test && npm run coverage && npm run sonar && npm run build",
    "coverage": "nyc report --reporter=lcov",
    "test": "nyc --reporter=html --reporter=text jest --runInBand"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bunyan": "^1.8.15",
    "jsonwebtoken": "^9.0.2",
    "pn-auth-common": "file:../authCommon"
  },
  "devDependencies": {
    "jest": "^30.0.2",
    "nyc": "^17.1.0",
    "sonarqube-scanner": "^4.3.0"
  }
}
